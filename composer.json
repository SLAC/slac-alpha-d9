{
  "name": "slac/slac-alpha-d9",
  "description": "Project template for Drupal projects with composer for pantheon",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "path",
      "url": "upstream-configuration"
    },
    {
      "type": "package",
      "package": {
        "name": "slac/slac",
        "version": "1.0.0-beta4",
        "dist": {
          "url": "https://github.com/SLAC/slac-drupal-profile-theme/releases/download/v1.0.0-beta.4/slac.zip",
          "type": "zip"
        },
        "type": "drupal-theme"
      }
    }
  ],
  "require": {
    "composer/installers": "^1.10",
    "cweagans/composer-patches": "^1.7",
    "drupal/address": "^1.11",
    "drupal/addtocal": "^1.1",
    "drupal/addtocal_augment": "^1.1@alpha",
    "drupal/admin_toolbar": "^3.1",
    "drupal/aggregator": "^1.0",
    "drupal/allowed_formats": "^1.3",
    "drupal/auto_entitylabel": "^3.0@beta",
    "drupal/better_exposed_filters": "^6.0",
    "drupal/block_field": "^1.0@RC",
    "drupal/components": "^2.4",
    "drupal/conditional_fields": "^4.0@alpha",
    "drupal/config_ignore": "^2.3",
    "drupal/config_pages": "^2.10",
    "drupal/config_split": "^1.7",
    "drupal/core-composer-scaffold": "^9",
    "drupal/core-recommended": "^9",
    "drupal/default_content": "^2.0@alpha",
    "drupal/devel": "^4.1",
    "drupal/duration_field": "^2.0",
    "drupal/easy_breadcrumb": "^2.0",
    "drupal/eca": "^1.0",
    "drupal/eca_cm": "^1.0",
    "drupal/encrypt": "^3.0",
    "drupal/entity_clone": "^1.0@beta",
    "drupal/entity_reference_revisions": "^1.9",
    "drupal/environment_indicator": "^4.0",
    "drupal/eu_cookie_compliance": "^1.19",
    "drupal/externalauth": "^2.0",
    "drupal/facets": "^2.0",
    "drupal/feeds": "^3.0@beta",
    "drupal/field_encrypt": "^3.0",
    "drupal/field_group": "^3.2",
    "drupal/focal_point": "^1.5",
    "drupal/fontawesome": "^2.24",
    "drupal/gin": "^3.0@beta",
    "drupal/gin_toolbar": "^1.0@beta",
    "drupal/google_tag": "^1.6",
    "drupal/key": "^1.16",
    "drupal/libraries": "^3.0@beta",
    "drupal/masquerade": "^2.0@RC",
    "drupal/maxlength": "^2.0@RC",
    "drupal/menu_admin_per_menu": "^1.5",
    "drupal/menu_block": "^1.8",
    "drupal/menu_item_extras": "^2.17",
    "drupal/metatag": "^1.16",
    "drupal/migrate_devel": "^2.0@alpha",
    "drupal/migrate_file": "^2.1",
    "drupal/migrate_manifest": "^3.0",
    "drupal/migrate_plus": "^6.0",
    "drupal/migrate_tools": "^6.0",
    "drupal/moderated_content_bulk_publish": "^2.0",
    "drupal/name": "^1.0@RC",
    "drupal/node_revision_delete": "^1.0@RC",
    "drupal/oembed_providers": "^2.0",
    "drupal/pantheon_advanced_page_cache": "^2.0",
    "drupal/paragraphs": "^1.14",
    "drupal/pathauto": "^1.11",
    "drupal/rabbit_hole": "^1.0@beta",
    "drupal/readonly_field_widget": "^1.5",
    "drupal/real_aes": "^2.4",
    "drupal/redirect": "^1.6",
    "drupal/redis": "^1.5",
    "drupal/rh_media": "^1.0@beta",
    "drupal/search_api": "^1.21",
    "drupal/sendgrid_integration": "^2.0",
    "drupal/simple_sitemap": "^3.11",
    "drupal/simplesamlphp_auth": "3.x-dev@dev",
    "drupal/single_content_sync": "^1.3",
    "drupal/site_settings": "^1.19",
    "drupal/sitewide_alert": "^2.0",
    "drupal/smart_date": "^3.6",
    "drupal/smart_trim": "^2.0",
    "drupal/style_options": "^1.0@alpha",
    "drupal/svg_image_field": "^2.1",
    "drupal/token_filter": "^1.4",
    "drupal/twig_field_value": "^2.0",
    "drupal/twig_tweak": "^3.1",
    "drupal/vapn": "^1.5",
    "drupal/view_mode_switch": "^2.0",
    "drupal/view_modes_display": "^2.3",
    "drupal/views_accordion": "^2.0",
    "drupal/views_bulk_operations": "^4.2",
    "drupal/views_rss": "^2.0@RC",
    "drupal/viewsreference": "^2.0@beta",
    "drupal/viewsreference_filter": "^1.0",
    "drupal/vppr": "^1.2",
    "drupal/webform": "^6.1",
    "drupal/workbench_access": "^1.1",
    "drupal/yaml_content": "^1.0@alpha",
    "drush/drush": "^10",
    "pantheon-systems/drupal-integrations": "^9",
    "rvtraveller/qs-composer-installer": "^1.2",
    "sainsburys/guzzle-oauth2-plugin": "^3.0",
    "simplesamlphp/simplesamlphp": " ^1.19",
    "slac/slac": "^1.0@alpha",
    "vlucas/phpdotenv": "^5.5"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
    "drupal/coder": "^8.3",
    "drupal/console": "^1.0.2",
    "drupal/core-dev": "^9",
    "phpmd/phpmd": "2.10.2",
    "phpspec/prophecy-phpunit": "^2",
    "squizlabs/php_codesniffer": "^2.7 || ^3"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "platform": {
      "ext-gd": "1.0.0",
      "ext-opcache": "1.0.0",
      "ext-pdo": "1.0.0"
    },
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "composer/installers": true,
      "drupal/core-composer-scaffold": true,
      "drupal/console-extend-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "rvtraveller/qs-composer-installer": true,
      "joachim-n/composer-manifest": true,
      "simplesamlphp/composer-module-installer": true
    }
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ],
    "files": [
      "load.environment.php"
    ]
  },
  "scripts": {
    "build-assets": [
      "composer install --optimize-autoloader --no-dev"
    ],
    "lint": "find web/modules/custom -name '*.php' -exec php -l {} \\;",
    "phpmd": [
      "phpmd . json phpmd.xml.dist --exclude \"web/private/*,modules/contrib/*,drush/*,vendor/*,web/core*,web/private/*,scripts/*\""
    ],
    "phpmd-local": [
      "@phpmd --reportfile phpmd-report.json"
    ],
    "code-sniff": [
      "./vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor ./web/modules/custom",
      "./vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor ./web/themes/slac",
      "./vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor ./web/modules/custom",
      "./vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor ./web/themes/slac"
    ],
    "unit-test": "echo 'No unit test step defined.'",
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
      "ln -sf ../../../../files/private/simplesamlphp/config vendor/simplesamlphp/simplesamlphp/config",
      "ln -sf ../../../../files/private/simplesamlphp/metadata vendor/simplesamlphp/simplesamlphp/metadata",
      "ln -sf ../../../../files/private/simplesamlphp/cert vendor/simplesamlphp/simplesamlphp/cert"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
      "ln -sf ../../../../files/private/simplesamlphp/config vendor/simplesamlphp/simplesamlphp/config",
      "ln -sf ../../../../files/private/simplesamlphp/metadata vendor/simplesamlphp/simplesamlphp/metadata",
      "ln -sf ../../../../files/private/simplesamlphp/cert vendor/simplesamlphp/simplesamlphp/cert"
    ],
    "post-create-project-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches-file": "composer.patches.json",
    "drupal-scaffold": {
      "allowed-packages": [
        "pantheon-systems/drupal-integrations"
      ],
      "locations": {
        "web-root": "web/"
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "harvesthq/chosen",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ]
    }
  }
}
