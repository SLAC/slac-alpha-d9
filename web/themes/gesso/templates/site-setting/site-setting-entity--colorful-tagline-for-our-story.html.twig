{% include '@components/tagline/tagline--long/tagline--long.twig' with {
  'tagline1_image': content.field_image|field_value,
  'tagline1': content.field_heading|field_value,
  'tagline1_content':  content.field_body|field_value,
  'tagline2_image': content.field_image_2|field_value,
  'tagline2': content.field_line_2|field_value,
  'tagline2_content': content.field_body_2|field_value,
  'tagline3_image': content.field_image_3|field_value,
  'tagline3': content.field_line_3|field_value,
  'tagline3_content': content.field_body_3|field_value
} %}

{% set catch_cache = content|render %}
