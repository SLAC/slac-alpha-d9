{% set section_content %}
{% include '@components/fifty-fifty/fifty-fifty.twig' with {
  'col_1': content.field_image,
  'col_2': content|without('field_image'),
  'modifier_classes': 'c-fifty-fifty--alt'
} %}
{% endset %}

{% include '@layouts/section/section.twig' with {
  'section_content': section_content,
  'has_constrain': false
} %}

{% set catch_cache = content|render %}
